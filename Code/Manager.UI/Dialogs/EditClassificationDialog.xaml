<Window x:Class="Manager.UI.Dialogs.EditClassificationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Классификация" WindowStartupLocation="CenterOwner" SizeToContent="WidthAndHeight">
	<DockPanel LastChildFill="True">
		<StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal">
			<Button Margin="2" Content="ОК" Click="buttonOkClick"/>
			<Button Margin="2" Content="Отмена" Click="buttonCancelClick"/>
		</StackPanel>
		<Grid Name="_classificationGrid">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="Auto" />
				<ColumnDefinition Width="*" MinWidth="150" />
			</Grid.ColumnDefinitions>
		</Grid>
	</DockPanel>
</Window>
